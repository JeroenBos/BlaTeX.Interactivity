{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "lint": "dts lint src test",
    "prepare": "dts build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4
  },
  "eslint": {
    "rules": {
      "prettier/prettier": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "jest": {
    "globals": {
      "transformIgnorePatterns": [".*"],
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "name": "template",
  "author": "Jeroen Bos",
  "module": "dist/template.esm.js",
  "devDependencies": {
    "@types/jsdom": "16.2.13",
    "dts-cli": "0.19.7",
    "husky": "7.0.4",
    "jsdom": "16.6.0",
    "tslib": "2.3.1",
    "tslint": "6.1.3",
    "typescript": "4.5.2",
    "katex": "0.11.1"
  }
}
